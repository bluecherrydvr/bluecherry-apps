/var/log/bluecherry.log
{
	rotate 7
	daily
	missingok
	notifempty
	delaycompress
	compress
	create 640 syslog bluecherry
	postrotate
		reload rsyslog >/dev/null 2>&1 || true
	endscript
}

/var/lib/bluecherry/www-*.log
{
	su www-data bluecherry
	rotate 2
	daily
	missingok
	notifempty
	postrotate
		apachectl graceful
	endscript
}
