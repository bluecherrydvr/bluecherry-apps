# Bluecherry configuration file

# Used to be sure we don't use configurations not suitable for us
version = "1.0";

bluecherry:
{
  db:
  {
    # 0 = sqlite, 1 = pgsql, 2 = mysql
    type = 2;
    dbname = "_DBC_DBNAME_";
    user = "_DBC_DBUSER_";
    password = "_DBC_DBPASS_";
    host = "_DBC_HOST_";
    userhost = "_DBC_USERHOST_";
  };
};
